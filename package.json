{
  "name": "ds-helper",
  "displayName": "ds-helper",
  "description": "a tool to help build page with dataSet batter",
  "publisher": "llzQAQ",
  "version": "0.2.0",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/LLZ66/ds-helper/tree/master"
  },
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ds-helper.helloWorld"
  ],
  "main": "./src/index.js",
  "contributes": {
    "commands": [
      {
        "command": "ds-helper.helloWorld",
        "title": "Hello World"
      }
    ],
    "keybindings": [
      {
        "command": "ds-helper.helloWorld",
        "key": "alt+d",
        "mac": "alt+d"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "ds-helper.helloWorld",
          "group": "navigation",
          "when": "editorHasSelection",
          "alt": "ds-helper.helloWorld"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "node ./test/runTest.js"
  },
  "dependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.69.0",
    "@vscode/test-electron": "^2.1.5",
    "acorn": "^8.8.0",
    "acorn-walk": "^8.2.0",
    "babel-loader": "^8.2.5",
    "escodegen": "^2.0.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^5.0.4"
  }
}
